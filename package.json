{
  "name": "crudlfap",
  "version": "0.4.0",
  "description": "CRUDLFA+ framework for Django 2.0",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "lint": "eslint js",
    "chrome-browser-test": "cd nightwatch && yarn test",
    "ie-browser-test": "cd nightwatch && yarn test -e internet_explorer",
    "firefox-browser-test": "cd nightwatch && yarn test -e firefox",
    "eslint-nightwatch": "cd nightwatch && yarn run lint",
    "browser-test": "cd nightwatch && yarn run test --config nightwatch-driver.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourlabs/crudlfap.git"
  },
  "keywords": [
    "django"
  ],
  "author": "âˆž <jpic@yourlabs.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourlabs/crudlfap/issues"
  },
  "homepage": "https://github.com/yourlabs/crudlfap#readme",
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "*{js,jsx}",
      "webpack.config.js"
    ]
  },
  "dependencies": {
    "next": "^6.0.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  }
}
